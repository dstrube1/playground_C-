<?xml version="1.0" encoding="utf-8" ?>
<xs:schema id="PatientData" targetNamespace="http://tempuri.org/PatientData.xsd" elementFormDefault="qualified"
	attributeFormDefault="qualified" xmlns="http://tempuri.org/PatientData.xsd" xmlns:mstns="http://tempuri.org/PatientData.xsd"
	xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata">
	<xs:element name="PatientData" msdata:IsDataSet="true">
		<xs:complexType>
			<xs:choice maxOccurs="unbounded">
				<xs:element name="Patient">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="CprID" type="xs:string" minOccurs="0" default="0" />
							<xs:element name="FirstName" type="xs:string" minOccurs="0" default="" />
							<xs:element name="LastName" type="xs:string" minOccurs="0" default="" />
							<xs:element name="MiddleName" type="xs:string" minOccurs="0" default="" />
							<xs:element name="BirthDate" type="xs:date" minOccurs="0" />
							<xs:element name="Height" type="xs:double" minOccurs="0" default="0" />
							<xs:element name="Weight" type="xs:double" minOccurs="0" default="0" />
							<xs:element name="Sex" type="xs:int" minOccurs="0" default="0" />
							<xs:element name="Race" type="xs:int" minOccurs="0" default="0" />
							<xs:element name="Technician" type="xs:string" minOccurs="0" default="" />
							<xs:element name="Physician" type="xs:string" minOccurs="0" default="" />
							<xs:element name="Smoke" type="xs:boolean" minOccurs="0" default="false" />
							<xs:element name="SmokePerDay" type="xs:int" minOccurs="0" default="0" />
							<xs:element name="SmokeDuration" type="xs:int" minOccurs="0" default="0" />
							<xs:element name="SmokeQuit" type="xs:boolean" minOccurs="0" default="false" />
							<xs:element name="SmokeQuitTime" type="xs:int" minOccurs="0" default="0" />
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:choice>
		</xs:complexType>
	</xs:element>
</xs:schema>